        </id>
        <property name="syIdSy" type="integer">
            <column name="sy_id_sy"/>
        </property>
        <property name="codigo" type="string">
            <column name="codigo" length="10"/>
        </property>
        <property name="param1" type="string">
            <column name="param1" length="50"/>
        </property>
        <property name="param2" type="string">
            <column name="param2" length="50"/>
        </property>
        <property name="param3" type="string">
            <column name="param3" length="50"/>
        </property>
        <property name="param4" type="string">
            <column name="param4" length="50"/>
        </property>
        <property name="param5" type="string">
            <column name="param5" length="50"/>
        </property>
    </class>
</hibernate-mapping>